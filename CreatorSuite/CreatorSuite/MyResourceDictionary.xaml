<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Resource dictionary entries should be defined here. -->
    <DataTemplate x:Key="SpellListTemplate" >
        <StackPanel Orientation="Vertical">
            <TextBlock Text="{Binding Name}" HorizontalAlignment="Stretch" Width="100"/>
        </StackPanel>
    </DataTemplate>
    <DataTemplate x:Key="ItemListTemplate" >
        <StackPanel Orientation="Vertical">
            <TextBlock Text="{Binding itemName}" HorizontalAlignment="Stretch" Width="100"/>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>